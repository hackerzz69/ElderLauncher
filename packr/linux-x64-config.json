{
    "platform": "linux64",
    "jdk": "linux-jdk",
    "executable": "OpenOSRS",
    "classpath": [
        "build/libs/OpenOSRS-shaded.jar"
    ],
    "mainclass": "net.runelite.launcher.Launcher",
    "vmargs": [
       "-Drunelite.launcher.nojvm=true",
       "-Xmx512m",
       "-Xss2m",
       "-XX:CompileThreshold=1500"
    ],
    "output": "native-linux-x86_64/OpenOSRS.AppDir/",
    "resources": [
      "build/filtered-resources/openosrs.desktop",
      "appimage/openosrs.png"
    ]
}
